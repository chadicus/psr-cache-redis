{
    "name": "chadicus/psr-cache-redis",
    "description": "PSR-16 SimpleCache Implementation using Predis",
    "keywords": ["psr16", "psr", "cache", "simple-cache", "redis", "predis"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chad Gray",
            "email": "chadwickgray@gmail.com",
            "role": "Developer"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": "^7.0",
        "psr/simple-cache": "^1.0",
        "predis/predis": "^1.1",
        "chadicus/psr-cache-helper": "^0.1"
    },
    "require-dev": {
        "chadicus/coding-standard": "^1.2",
        "php-coveralls/php-coveralls": "^1.0",
        "phpunit/phpunit": "^6.1"
    },
    "autoload": {
        "psr-4": {
            "Chadicus\\Psr\\SimpleCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ChadicusTest\\Psr\\SimpleCache\\": "tests"
        }
    }
}
